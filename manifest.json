{
  "manifest_version":2,
  "name": "conterium",
  "version": "0.1",
  "options_ui": {
      "page": "options.html",
      "chrome_style": true
  },
  "permissions": [
      "contextMenus",
      "http://*/*",
      "https://*/*",
      "storage"
  ],
  "icons": {
    "16": "img-16.png"
  },
  "background": {
      "persistent": false,
      "scripts": ["src/jquery.min.js","src/main.js"]
  }
}